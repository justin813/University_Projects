	START	100
	LDX	MEM	.MEM = MEM USED IN RESW, HERE IT IS 30
	LDA   	#3	.A = 3 = NUM TIMES LOOP FOR ARTHIMETIC
	LDB   	ZERO     	.B = 0 = ENDING POINT FOR THE LOOP
	LDS   	NUMS,X 	.LOADS STORAGE X THE NUMS STORED
	STS	INV    	.INV HOLDS THE RESW LIST
LOP	SUBR  	A,X    	.START OF LOOP, A IS THE ACCUMLATOR TO LOOP BACKWARDS
	LDS   	NUMS,X 
	RMO   	X,T    	.CARRY THE INDEX FROM REGISTER X INTO T
	ADDR  	A,B    	.SINCE B = 0 WE ADD A'S VALUE TO STORE A
	RMO   	B,X    	.THEN WE LOAD X WITH VALUE B
	STS   	INV,X  	.STORE X INTO LIST
	RMO   	T,X    	.NOW WE LOAD T INTO X FOR THE INDEX
	LDT   	ZERO   	.THEN RESET REGISTER T
	COMPR 	X,T    
	JGT   	LOP
	RSUB
ZERO	WORD  	0
MEM	WORD  	30
NUMS	WORD  	93
	WORD	50
	WORD  	197
	WORD	55
	WORD  	96
	WORD  	65
	WORD  	74
	WORD  	92
	WORD  	43
	WORD  	8
	WORD	6
INV	RESW  	10
	END
